class HelloWorld
{
    // globals
    // i
    final int i = 1;

    // main function
    public void main() {

        int x;
        int y;

        x = 4;
        y = 5;

        if( x == y )
        {
            x = 1;
        }
        else{
            x = 2;
        }

        while( x <= y){
            x = addI(2);
        }

    }

    // add method
    public int addI(int z) {
        return z + i;
    }
}