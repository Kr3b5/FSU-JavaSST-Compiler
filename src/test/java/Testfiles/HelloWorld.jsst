class HelloWorld
{
    // globals
    // i
    final int i = 1;

    // main function
    public void main() {

        int x;
        int y;
        x = 4;
        y = 5;

        if( x <= y ){ addI(x); }
        if( x < y ){ addI(x); }
        if( x >= y ){ addI(y); }
        if( x > y ){ addI(y); }

        if( x == y ){ addI(1);  }
        else{
            addI(0);
        }

        while( x != y){
            x + 1;
        }

    }

    // add method
    public int addI(int z) {
        return z + i;
    }
}